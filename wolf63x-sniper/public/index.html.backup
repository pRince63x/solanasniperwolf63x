    <script>
        document.addEventListener('DOMContentLoaded', async () => {
            // Initialize the application
            const app = new SniperApp();
            await app.initialize();
            
            // Handle compact sell buttons in active trades
            document.querySelectorAll('#active-trades-list .compact-sell-btn').forEach(btn => {
                btn.addEventListener('click', function() {
                    const tokenName = this.closest('.compact-trade-item').querySelector('.compact-token-name').textContent;
                    console.log(`Selling token from active trades: ${tokenName}`);
                    
                    // Show a confirmation notification
                    showNotification(`Selling ${tokenName}...`, 'info');
                    
                    // Simulate selling process
                    setTimeout(() => {
                        showNotification(`Successfully sold ${tokenName}!`, 'success');
                        // Remove the item after successful sale
                        this.closest('.compact-trade-item').remove();
                        // Update counts
                        updateTradeCountsInDashboard();
                    }, 1500);
                });
            });
            
            // Function to update trade counts in dashboard
            function updateTradeCountsInDashboard() {
                const totalTrades = document.querySelectorAll('#active-trades-list .compact-trade-item').length;
                const profitTrades = document.querySelectorAll('#active-trades-list .compact-trade-item .compact-profit.positive').length;
                
                // Update See all link in dashboard
                const seeAllLink = document.querySelector('.see-all-trades');
            }
            
            // Function to sync open trades data from sniper menu to dashboard
            function syncOpenTradesToDashboard() {
                // Update trade counts
                updateTradeCountsInDashboard();
                
                const openTradesItems = document.querySelectorAll('#active-trades-list .compact-trade-item');
                const dashboardTradesBody = document.getElementById('dashboard-trades-body');
                
                if (!dashboardTradesBody) return;
                
                // Clear existing dashboard trades
                dashboardTradesBody.innerHTML = '';
                
                // Count variables
                let totalTrades = openTradesItems.length;
                let profitCount = 0;
                let lossCount = 0;
                let totalProfit = 0;
                let totalLoss = 0;
                
                // Populate dashboard trades table
                openTradesItems.forEach(item => {
                    const tokenName = item.querySelector('.compact-token-name')?.textContent || 'Unknown';
                    const entryPrice = item.querySelector('.compact-entry')?.textContent || '$0.00';
                    const currentPrice = item.querySelector('.compact-current')?.textContent || '$0.00';
                    const profitElement = item.querySelector('.compact-profit');
                    const profit = profitElement?.textContent || '$0.00';
                    const isProfit = profitElement?.classList.contains('positive');
                    const time = item.querySelector('.compact-time')?.textContent || '0m';
                    
                    // Create new row for dashboard
                    const newRow = document.createElement('tr');
                    newRow.innerHTML = `
                        <td><span class="token-symbol">${tokenName}</span></td>
                        <td>${entryPrice}</td>
                        <td>${currentPrice}</td>
                        <td class="${isProfit ? 'positive' : 'negative'}">${profit}</td>
                        <td>${time}</td>
                        <td>
                            <button class="btn btn-sm btn-success sell-btn">Sell</button>
                        </td>
                    `;
                    
                    // Add click event to sell button
                    const sellBtn = newRow.querySelector('.sell-btn');
                    sellBtn.addEventListener('click', function() {
                        // Find the original sell button in the sniper menu and trigger its click
                        const originalSellBtn = item.querySelector('.compact-sell-btn');
                        if (originalSellBtn) {
                            originalSellBtn.click();
                        } else {
                            console.log(`Selling ${tokenName} from dashboard`);
                            showNotification(`Selling ${tokenName}...`, 'info');
                            
                            // Simulate selling process
                            setTimeout(() => {
                                showNotification(`Successfully sold ${tokenName}!`, 'success');
                                newRow.remove();
                                updateTradeCountsInDashboard(); // Re-sync after selling
                            }, 1500);
                        });
                    }
                    
                    // Add row to dashboard
                    dashboardTradesBody.appendChild(newRow);
                    
                    // Update counts
                    updateTradeCountsInDashboard();
                    }
                });
                
                // Update dashboard counts and amounts
                document.getElementById('dashboard-open-trades-count').textContent = totalTrades;
                document.getElementById('dashboard-profit-count').textContent = profitCount;
                document.getElementById('dashboard-loss-count').textContent = lossCount;
                document.getElementById('dashboard-profit-amount').textContent = `$${totalProfit.toFixed(2)}`;
                document.getElementById('dashboard-loss-amount').textContent = `$${Math.abs(totalLoss).toFixed(2)}`;
                
                // Update chart value based on total profit/loss
                const totalPnL = totalProfit + totalLoss;
                const chartValue = document.getElementById('chart-current-value');
                if (chartValue) {
                    chartValue.textContent = totalPnL >= 0 ? `+$${totalPnL.toFixed(2)}` : `-$${Math.abs(totalPnL).toFixed(2)}`;
                    chartValue.className = totalPnL >= 0 ? 'chart-value positive' : 'chart-value negative';
                }
                
                // Update See all link in dashboard if it exists
                const seeAllLink = document.querySelector('.see-all-trades');
                if (seeAllLink) {
                    seeAllLink.textContent = `See all (${totalTrades})`;
                }
            }
            
            // Initialize dashboard trade counts and sync open trades
            syncOpenTradesToDashboard();
            
            // Set up event listeners for trade updates
            document.addEventListener('tradesUpdated', function() {
                syncOpenTradesToDashboard();
            });
            
            // Set up event listener for dashboard take all profits button
            const dashboardTakeAllProfitsBtn = document.getElementById('dashboard-take-all-profits');
            if (dashboardTakeAllProfitsBtn) {
                dashboardTakeAllProfitsBtn.addEventListener('click', function() {
                    // Find the original take all profits button in the sniper menu and trigger its click
                    const originalTakeAllProfitsBtn = document.querySelector('#take-all-profits-btn');
                    if (originalTakeAllProfitsBtn) {
                        originalTakeAllProfitsBtn.click();
                    } else {
                        console.log('Taking all profits from dashboard');
                        showNotification('Taking all profits...', 'info');
                        
                        // Simulate taking all profits
                        setTimeout(() => {
                            showNotification('Successfully took all profits!', 'success');
                            syncOpenTradesToDashboard();
                        }, 1500);
                    }
                });
            }
            
            // Set up random character animations
            function setupCharacterAnimations() {
                const character = document.querySelector('.cartoon-character');
                if (!character) return;                    const animations = [
                    'character-jumping',
                    'character-waving',
                    'character-spinning',
                    'character-phone',
                    'character-eating',
                    'character-sleeping',
                    'character-shooting',
                    'character-dancing',
                    'character-smoking',
                    'character-swimming',
                    'character-walking',
                    'character-driving',
                    'character-parachuting',
                    'character-gaming',
                    'character-jumping',
                    'character-flipping',
                    'character-sitting'
                ];
                let currentAnimation = '';
                
                function triggerRandomAnimation() {
                    // Don't animate if character is already in a special state
                    if (character.classList.contains('running-animation') || 
                        character.classList.contains('running-back') ||
                        character.classList.contains('collecting')) {
                        // Try again later
                        setTimeout(triggerRandomAnimation, 3000);
                        return;
                    }
                    
                    // Remove any existing animation classes
                    animations.forEach(anim => {
                        character.classList.remove(anim);
                    });
                    
                    // 30% chance to not do any animation
                    if (Math.random() < 0.3) {
                        setTimeout(triggerRandomAnimation, 2000 + Math.random() * 5000);
                        return;
                    }
                    
                    // Select random animation
                    const animationClass = animations[Math.floor(Math.random() * animations.length)];
                    character.classList.add(animationClass);
                    
                    // Set timeout to remove animation class
                    setTimeout(() => {
                        character.classList.remove(animationClass);
                        // Schedule next animation
                        setTimeout(triggerRandomAnimation, 2000 + Math.random() * 5000);
                    }, 3000 + Math.random() * 2000);
                }
                
                // Start animation cycle
                triggerRandomAnimation();
            }
                    
                    // For special animations, add custom behavior
                    if (animationClass === 'character-shooting') {
                        // Add sound effect or visual indicators for shooting
                        const profitDisplay = document.querySelector('.profit-display');
                        if (profitDisplay) {
                            profitDisplay.classList.add('highlight');
                            profitDisplay.style.transition = 'transform 0.1s';
                            setTimeout(() => {
                                profitDisplay.style.transform = 'translateX(2px)';
                                setTimeout(() => {
                                    profitDisplay.classList.remove('highlight');
                                    profitDisplay.style.transform = '';
                                }, 1000);
                            }, 100);
                        }
                    }
                    
                    // Duration for this animation
                    const duration = 1000 + Math.random() * 3000;
                    
                    // Schedule removal of animation class
                    setTimeout(() => {
                        character.classList.remove(animationClass);
                        
                        // Schedule next animation
                        setTimeout(triggerRandomAnimation, 3000 + Math.random() * 7000);
                    }, duration);
                }
                
                // Initial delay before starting animations
                setTimeout(triggerRandomAnimation, 5000);
            }
            
            // Initialize character animations
            setupCharacterAnimations();
            
            // Update the Take Profits button functionality in dashboard
            document.querySelector('.take-profits-btn')?.addEventListener('click', function() {
                // Find all trades in profit in dashboard
                const profitableTrades = document.querySelectorAll('#active-trades-list .compact-trade-item .compact-profit.positive');
                
                if (profitableTrades.length === 0) {
                    showNotification('No profitable trades to close!', 'warning');
                    return;
                }
                
                // Show confirmation
                showNotification(`Closing ${profitableTrades.length} profitable trades...`, 'info');
                
                // Animate the cartoon character
                animateCharacterCollectingProfits();
                
                // Simulate closing process
                setTimeout(() => {
                    profitableTrades.forEach(profitEl => {
                        const tradeItem = profitEl.closest('.compact-trade-item');
                        if (tradeItem) {
                            tradeItem.remove();
                        }
                    });
                    
                    // Update counts
                    updateTradeCountsInDashboard();
                    
                    // Show success
                    showNotification(`Successfully closed ${profitableTrades.length} profitable trades!`, 'success');
                }, 3000); // Extended timeout to allow for animation
            });
            
            // Function to animate the character collecting profits
            function animateCharacterCollectingProfits() {
                // Get the character, profit amount and today's earned elements
                const character = document.querySelector('.cartoon-character');
                const profitAmount = document.querySelector('.profit-amount');
                const profitText = profitAmount.textContent;
                const profitValue = parseFloat(profitText.replace(/[^0-9.-]+/g, ""));
                const todaysEarnedBtn = document.querySelector('.todays-earned-btn');
                const earnedAmount = document.querySelector('.earned-amount');
                
                // Get positions for animation
                const characterRect = character.getBoundingClientRect();
                const earnedRect = todaysEarnedBtn.getBoundingClientRect();
                
                // Calculate the current earned amount
                let currentEarned = parseFloat(earnedAmount.textContent.replace(/[^0-9.-]+/g, "")) || 0;
                
                // Create a floating profit text that will move with the character
                const floatingProfit = document.createElement('div');
                floatingProfit.className = 'floating-profit';
                floatingProfit.textContent = profitText;
                floatingProfit.style.position = 'absolute';
                floatingProfit.style.color = '#4CAF50';
                floatingProfit.style.fontWeight = 'bold';
                floatingProfit.style.fontSize = '14px';
                floatingProfit.style.zIndex = '1000';
                document.body.appendChild(floatingProfit);
                
                // Position the floating profit initially near the character
                positionElement(floatingProfit, characterRect.left + 20, characterRect.top);
                
                // Add "collecting" animation to the character
                character.classList.add('collecting');
                
                // First animation phase: character bends to pick up profit
                setTimeout(() => {
                    // Make profit amount appear to be picked up (fade out)
                    profitAmount.style.opacity = '0';
                    
                    // Create money particles for effect
                    createMoneyParticles(characterRect.left, characterRect.top);
                    
                    // Second animation phase: character runs to Today's Earned with profit
                    setTimeout(() => {
                        // Character starts running
                        character.classList.remove('collecting');
                        character.classList.add('running-animation');
                        
                        // Floating profit follows character
                        const runDuration = 1500; // match this to the CSS animation duration
                        animateElement(
                            floatingProfit, 
                            characterRect.left + 20, 
                            characterRect.top,
                            earnedRect.left, 
                            earnedRect.top,
                            runDuration
                        );
                        
                        // Third animation phase: profit is added to Today's Earned
                        setTimeout(() => {
                            // Add profit to earned amount
                            currentEarned += profitValue;
                            earnedAmount.textContent = `$${currentEarned.toFixed(2)}`;
                            earnedAmount.classList.add('highlight');
                            
                            // Create money particles at Today's Earned
                            createMoneyParticles(earnedRect.left + earnedRect.width/2, earnedRect.top + earnedRect.height/2, 5);
                            
                            // Remove floating profit
                            floatingProfit.remove();
                            
                            // Final animation phase: character runs back
                            setTimeout(() => {
                                // Reset character position and classes for the run back
                                character.classList.remove('running-animation');
                                character.style.transform = 'scaleX(-1)'; // Flip character
                                
                                // Create a new animation for running back
                                character.classList.add('running-back');
                                
                                // After character returns
                                setTimeout(() => {
                                    // Reset everything
                                    character.classList.remove('running-back');
                                    character.style.transform = '';
                                    earnedAmount.classList.remove('highlight');
                                    profitAmount.style.opacity = '1';
                                }, 1500);
                            }, 500);
                        }, runDuration);
                    }, 500);
                }, 300);
            }
            
            // Helper function to create money particles
            function createMoneyParticles(x, y, count = 10) {
                for (let i = 0; i < count; i++) {
                    const particle = document.createElement('div');
                    particle.className = 'money-particle';
                    particle.style.position = 'absolute';
                    particle.style.left = `${x}px`;
                    particle.style.top = `${y}px`;
                    
                    // Add some randomness
                    const randomX = Math.random();
                    const randomRotate = Math.random();
                    particle.style.setProperty('--random-x', randomX);
                    particle.style.setProperty('--random-rotate', randomRotate);
                    
                    document.body.appendChild(particle);
                    
                    // Remove particle after animation completes
                    setTimeout(() => {
                        particle.remove();
                    }, 2000);
                }
            }
            
            // Helper function to position an element
            function positionElement(element, x, y) {
                element.style.left = `${x}px`;
                element.style.top = `${y}px`;
            }
            
            // Helper function to animate an element from one position to another
            function animateElement(element, startX, startY, endX, endY, duration) {
                const startTime = performance.now();
                
                function updatePosition(currentTime) {
                    const elapsed = currentTime - startTime;
                    const progress = Math.min(elapsed / duration, 1);
                    
                    // Easing function for smoother animation
                    const easeProgress = progress < 0.5 
                        ? 4 * progress * progress * progress 
                        : 1 - Math.pow(-2 * progress + 2, 3) / 2;
                    
                    const currentX = startX + (endX - startX) * easeProgress;
                    const currentY = startY + (endY - startY) * easeProgress;
                    
                    positionElement(element, currentX, currentY);
                    
                    if (progress < 1) {
                        requestAnimationFrame(updatePosition);
                    }
                }
                
                requestAnimationFrame(updatePosition);
            }
            
            // Function to create money particles
            function createMoneyParticle(rect) {
                const particle = document.createElement('div');
                particle.className = 'money-particle';
                
                // Random position around the profit amount
                const randomX = Math.random();
                const randomY = Math.random();
                const randomRotate = Math.random();
                
                particle.style.setProperty('--random-x', randomX.toString());
                particle.style.setProperty('--random-rotate', randomRotate.toString());
                
                particle.style.left = `${rect.left + rect.width * randomX}px`;
                particle.style.top = `${rect.top + rect.height * randomY}px`;
                
                document.body.appendChild(particle);
                
                // Remove particle after animation
                setTimeout(() => {
                    document.body.removeChild(particle);
                }, 2000);
            }
            
            // Function to show notifications
            function showNotification(message, type = 'info') {
                const notification = document.createElement('div');
                notification.className =  otification ${type}`;
                notification.textContent = message;
                
                const container = document.getElementById('notification-container');
                if (container) {
                    container.appendChild(notification);
                    
                    // Show notification
                    setTimeout(() => {
                        notification.classList.add('active');
                    }, 10);
                    
                    // Remove after duration
                    setTimeout(() => {
                        notification.classList.remove('active');
                        setTimeout(() => {
                            notification.remove();
                        }, 300);
                    }, 3000);
                }
            }
            
            // Function to update trade counts in sniper page
            function updateSniperTradeCounts() {
                const totalTrades = document.querySelectorAll('.compact-trade-item').length;
                const profitTrades = document.querySelectorAll('.compact-trade-item .compact-profit.positive').length;
                const lossTrades = totalTrades - profitTrades;
                
                // Update counts in UI
                const openTradesCount = document.getElementById('open-trades-count');
                const profitCount = document.getElementById('profit-count');
                const lossCount = document.getElementById('loss-count');
                
                if (openTradesCount) openTradesCount.textContent = totalTrades;
                if (profitCount) profitCount.textContent = profitTrades;
                if (lossCount) lossCount.textContent = lossTrades;
            }
            
            // Initialize sniper page counts
            updateSniperTradeCounts();
            
            // Handle "See all" link in active trades
            document.querySelector('.see-all-trades')?.addEventListener('click', (e) => {
                // Navigate to sniper page
                document.querySelector('a[href="#sniper"]').click();
                
                // Optionally scroll to open trades section
                setTimeout(() => {
                    const openTradesSection = document.querySelector('.open-trades');
                    if (openTradesSection) {
                        openTradesSection.scrollIntoView({ behavior: 'smooth' });
                    }
                }, 100);
            });
            
            // Initialize bot toggle switch
            const botToggle = document.getElementById('bot-status-toggle');
            if (botToggle) {
                // Check if bot status was saved in localStorage
                const savedStatus = localStorage.getItem('botStatus');
                if (savedStatus === 'active') {
                    botToggle.checked = true;
                    document.getElementById('bot-status-text').textContent = 'ON';
                    document.getElementById('bot-status-text').style.color = 'var(--toggle-active)';
                }
                
                // Add event listener for toggle change
                botToggle.addEventListener('change', (e) => {
                    const isActive = e.target.checked;
                    const statusText = document.getElementById('bot-status-text');
                    
                    // Update UI
                    if (statusText) {
                        statusText.textContent = isActive ? 'ON' : 'OFF';
                        statusText.style.color = isActive ? 'var(--toggle-active)' : 'var(--text-secondary)';
                    }
                    
                    // Save status to localStorage
                    localStorage.setItem('botStatus', isActive ? 'active' : 'inactive');
                    
                    // Show notification
                    const notification = document.createElement('div');
                    notification.className =  otification ${isActive ? 'success' : 'info'}`;
                    notification.textContent = `Bot is now ${isActive ? 'active' : 'inactive'}`;
                    document.body.appendChild(notification);
                    
                    // Show notification
                    setTimeout(() => {
                        notification.classList.add('show');
                    }, 10);
                    
                    // Remove after duration
                    setTimeout(() => {
                        notification.classList.remove('show');
                        setTimeout(() => {
                            document.body.removeChild(notification);
                        }, 300);
                    }, 3000);
                    
                    // If active, start the bot operations
                    if (isActive) {
                        console.log('Starting bot operations...');
                        // Add your bot start logic here
                    } else {
                        console.log('Stopping bot operations...');
                        // Add your bot stop logic here
                    }
                });
            }
            
            // Sync open trades with active trades section
            function syncActiveTrades() {
                // Get open trades from the sniper page table
                const openTradesTable = document.getElementById('open-trades-table');
                const activeTradesContainer = document.querySelector('.active-trades-container');
                
                if (openTradesTable && activeTradesContainer) {
                    // Clear current active trades
                    activeTradesContainer.innerHTML = '';
                    
                    // Get all rows from open trades table
                    const tradeRows = openTradesTable.querySelectorAll('tr');
                    
                    if (tradeRows.length === 0) {
                        // No trades, show empty state
                        const emptyState = document.createElement('div');
                        emptyState.className = 'empty-state';
                        emptyState.textContent = 'No active trades at the moment';
                        activeTradesContainer.appendChild(emptyState);
                        return;
                    }
                    
                    // Process each trade row
                    tradeRows.forEach(row => {
                        const cells = row.querySelectorAll('td');
                        if (cells.length >= 4) { // Make sure there are enough cells
                            const tokenName = cells[0].textContent.trim();
                            const profitText = cells[3].textContent.trim();
                            const isProfitable = !profitText.includes('-');
                            
                            // Create token first letter for the icon
                            const tokenFirstLetter = tokenName.charAt(0);
                            
                            // Create trade card
                            const tradeCard = document.createElement('div');
                            tradeCard.className = 'active-trade-card';
                            
                            tradeCard.innerHTML = `
                                <div class="trade-token">
                                    <div class="token-icon">${tokenFirstLetter}</div>
                                    <div class="token-name">${tokenName}</div>
                                </div>
                                <div class="trade-profit ${isProfitable ? 'positive' : 'negative'}">${profitText}</div>
                                <div class="trade-actions">
                                    <button class="trade-action-btn sell-action">Sell</button>
                                    <button class="trade-action-btn view-action">View</button>
                                </div>
                            `;
                            
                            // Add event listeners for buttons
                            const sellBtn = tradeCard.querySelector('.sell-action');
                            const viewBtn = tradeCard.querySelector('.view-action');
                            
                            if (sellBtn) {
                                sellBtn.addEventListener('click', () => {
                                    // Find and click the corresponding sell button in the open trades table
                                    const sellBtnInTable = row.querySelector('.sell-btn');
                                    if (sellBtnInTable) {
                                        sellBtnInTable.click();
                                    }
                                });
                            }
                            
                            if (viewBtn) {
                                viewBtn.addEventListener('click', () => {
                                    // Find and click the corresponding view button in the open trades table
                                    const viewBtnInTable = row.querySelector('.view-btn');
                                    if (viewBtnInTable) {
                                        viewBtnInTable.click();
                                    }
                                });
                            }
                            
                            // Add card to container
                            activeTradesContainer.appendChild(tradeCard);
                        }
                    });
                    
                    // Add event listener to Sell All button
                    const sellAllBtn = document.querySelector('.sell-all-btn');
                    if (sellAllBtn) {
                        sellAllBtn.addEventListener('click', () => {
                            // Find and click the Take All Profits button in the open trades section
                            const takeAllProfitsBtn = document.getElementById('take-all-profits');
                            if (takeAllProfitsBtn) {
                                takeAllProfitsBtn.click();
                            }
                        });
                    }
                }
            }
            
            // Sync active trades initially
            syncActiveTrades();
            
            // Set up interval to sync every 5 seconds
