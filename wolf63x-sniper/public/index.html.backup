    <style>
        /* Custom CSS to override the header layout */
        body {
            overflow: hidden; /* Prevent scrolling of the body */
            margin: 0;
            padding: 0;
        }
        
        :root {
            --accent-color-rgb: 87, 202, 132; /* RGB values for green accent color */
        }
        
        .app-container {
            position: relative;
            height: 100vh;
            overflow: hidden;
            margin: 0;
            padding: 0;
        }
        
        /* Seamless UI with no borders */
        .top-bar {
            background-color: var(--bg-dark) !important;
            backdrop-filter: none !important;
            border: none !important; /* Remove border */
            padding: 0 0.5rem 0 1rem !important; /* Reduce padding */
            height: 55px !important; /* Increased height to fit larger logo */
            position: fixed !important;
            top: 0 !important;
            left: 0 !important;
            width: 100% !important;
            z-index: 100 !important;
            box-sizing: border-box !important;
            box-shadow: none !important; /* Remove shadow */
            display: flex !important;
            align-items: center !important;
        }
        
        .top-bar-logo {
            height: 50px !important; /* Increased size */
            margin-right: 10px !important; /* Reduced margin to bring search closer */
            display: inline-flex !important;
            filter: drop-shadow(0 0 3px rgba(48, 209, 88, 0.5)) !important; /* Green glow effect */
            align-self: center !important;
            transition: all 0.3s ease !important;
        }
        
        /* Hover effect removed as requested */
        
        .sidebar {
            background-color: var(--bg-dark) !important;
            border: none !important; /* Remove border */
            position: fixed !important;
            top: 55px !important; /* Position below taller header */
            left: 0 !important;
            z-index: 90 !important;
            width: 80px !important;
            height: calc(100vh - 55px) !important; /* Adjust for taller header */
            padding-top: 0 !important; /* Remove top padding completely */
            box-sizing: border-box !important;
            display: flex !important;
            flex-direction: column !important;
            box-shadow: none !important; /* Remove shadow */
        }
        
        .main-content {
            position: fixed !important;
            top: 55px !important; /* Adjusted for taller header */
            left: 80px !important; /* Sidebar width */
            width: calc(100vw - 80px) !important;
            height: calc(100vh - 55px) !important; /* Adjust for taller header */
            padding: 0 !important; /* Remove ALL padding */
            margin: 0 !important;
            overflow-y: auto !important;
            overflow-x: hidden !important;
            background-color: var(--bg-dark) !important;
            z-index: 80 !important;
            box-sizing: border-box !important;
        }
        
        /* Remove all borders and box styles from cards and containers */
        .card, .glass-card, .dashboard-card, .sniper-controls, .open-trades,
        .market-scanner, .pairs-container, .pairs-filters-card,
        .portfolio-container, .history-container, .settings-container {
            background-color: var(--bg-dark) !important;
            border: none !important;
            border-radius: 0 !important;
            box-shadow: none !important;
            padding: 0 0.5rem !important; /* No top padding, small side padding */
            margin: 0 0 0.5rem 0 !important; /* No top margin */
        }
        
        /* Remove card hover effects */
        .card:hover, .glass-card:hover, .dashboard-card:hover {
            transform: none !important;
            box-shadow: none !important;
        }
        
        /* Remove card before/after decorative elements */
        .card::before, .glass-card::before, .dashboard-card::before,
        .card::after, .glass-card::after, .dashboard-card::after {
            display: none !important;
        }
        
        /* Remove all internal borders */
        .trades-table th, .scanner-table th,
        .trades-table td, .scanner-table td,
        .chart-header, .dashboard-header, .active-trades-header,
        .pairs-header, .settings-section {
            border: none !important;
            border-bottom: none !important;
            border-top: none !important;
        }
        
        /* Make sure all content containers don't add extra space */
        .content-container {
            padding: 0 !important;
            margin: 0 !important;
            box-sizing: border-box !important;
            max-width: 100% !important;
            width: 100% !important;
            height: auto !important;
            overflow: visible !important;
        }
        
        /* Ensure pages fit within viewport */
        .app-page, .page {
            padding: 0 !important;
            margin: 0 !important;
            box-sizing: border-box !important;
            max-width: 100% !important;
            width: 100% !important;
            height: auto !important;
        }
        
        /* Ensure all containers don't overflow */
        .sniper-controls, .open-trades, .dashboard-stats, 
        .dashboard-content, .chart-section, .active-trades-section {
            box-sizing: border-box !important;
            max-width: 100% !important;
            margin: 0 0 0.5rem 0 !important; /* No top margin */
            overflow: hidden !important;
        }
        
        /* Dashboard header completely hidden or no spacing */
        .dashboard-header {
            display: none !important; /* Option 1: Hide completely */
            /* Option 2: If keeping it, ensure no space: */
            /* padding: 0 !important;
            margin: 0 !important;
            height: 0 !important; */
        }
        
        /* Stat cards with minimal styling */
        .stat-card {
            background-color: var(--bg-input) !important;
            border: none !important;
            border-radius: 0 !important;
            padding: 0.5rem !important;
            margin: 0 !important;
        }
        
        /* Chart containers need explicit dimensions */
        .chart-container {
            width: 100% !important;
            height: 300px !important;
            overflow: hidden !important;
            background-color: var(--bg-dark) !important;
            border: none !important;
        }
        
        /* Hide section titles in main UI */
        .dashboard-header h2, 
        .pairs-header h2,
        .section-title,
        .page-title {
            display: none !important;
        }
        
        /* Bot status toggle in active trades section */
        .bot-status-control {
            display: flex !important;
            align-items: center !important;
            gap: 8px !important;
            margin-right: 15px !important;
            background: transparent !important;
            padding: 0 !important;
        }
        
        .bot-status-label {
