                    // Add active class to clicked filter
                    this.classList.add('active');
                    
                    const filterType = this.getAttribute('data-filter');
                    
                    // Filter trade items
                    tradeItems.forEach(item => {
                        const pnlCell = item.querySelector('.pnl-cell');
                        const isProfitable = pnlCell.classList.contains('positive');
                        
                        if (filterType === 'all') {
                            item.style.display = 'grid';
                        } else if (filterType === 'profit' && isProfitable) {
                            item.style.display = 'grid';
                        } else if (filterType === 'loss' && !isProfitable) {
                            item.style.display = 'grid';
                        } else if (filterType === 'recent') {
                            // For demo purposes, just show the top 5 trades
                            const index = Array.from(tradeItems).indexOf(item);
                            item.style.display = index < 5 ? 'grid' : 'none';
                        } else {
                            item.style.display = 'none';
                        }
                    });
                });
            });
        }
        
        function setupChartTypeButtons() {
            const chartTypeButtons = document.querySelectorAll('.chart-type-btn');
            
            chartTypeButtons.forEach(button => {
                button.addEventListener('click', function() {
                    // Remove active class from all buttons
                    chartTypeButtons.forEach(btn => btn.classList.remove('active'));
                    // Add active class to clicked button
                    this.classList.add('active');
                    
                    const chartType = this.getAttribute('data-type');
                    
                    // Update chart type
                    if (window.portfolioChart) {
                        // Show loading overlay
                        document.getElementById('chart-overlay').classList.add('active');
                        
                        setTimeout(() => {
                            // Change chart type
                            window.portfolioChart.config.type = chartType;
                            // Update chart
                            window.portfolioChart.update();
                            
                            // Hide loading overlay
                            document.getElementById('chart-overlay').classList.remove('active');
                        }, 500);
                    }
                });
            });
        }
        
        function setupChartControls() {
            const toggleAnnotations = document.getElementById('toggle-annotations');
            const toggleTrades = document.getElementById('toggle-trades');
            
            if (toggleAnnotations) {
                toggleAnnotations.addEventListener('click', function() {
                    this.classList.toggle('active');
                    
                    // In a real app, you would toggle annotations on the chart
                    // For demo, we'll just show a notification
                    alert('Annotations ' + (this.classList.contains('active') ? 'enabled' : 'disabled'));
                });
            }
            
            if (toggleTrades) {
                toggleTrades.addEventListener('click', function() {
                    this.classList.toggle('active');
                    
                    // In a real app, you would toggle trade markers on the chart
                    // For demo, we'll just show a notification
                    alert('Trade markers ' + (this.classList.contains('active') ? 'enabled' : 'disabled'));
                });
            }
        }
    </script>
    
   
