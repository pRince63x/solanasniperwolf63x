        }
        
        .trade-item {
            display: grid;
            grid-template-columns: 1fr 1fr 1fr 1fr 1fr;
            padding: 16px;
            border-bottom: 1px solid rgba(255, 255, 255, 0.05);
            transition: background-color 0.2s ease;
        }
        
        .trade-item:hover {
            background-color: rgba(255, 255, 255, 0.05);
        }
        
        .token-name {
            font-weight: 600;
            color: #fff;
            display: flex;
            align-items: center;
            gap: 8px;
        }
        
        .token-icon {
            width: 20px;
            height: 20px;
            border-radius: 50%;
