    <style>
        /* Dashboard styling from dashboard.html */
        .dashboard {
            display: flex;
            height: calc(100vh - 200px);
            width: 100%;
            max-height: 700px;
        }
        
        /* Open Trades Panel */
        .trades-panel {
            width: 40%;
            border-right: 1px solid rgba(255, 255, 255, 0.05);
            display: flex;
            flex-direction: column;
            background-color: #1a1a1a;
            box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
            max-height: 100%;
            overflow: hidden;
        }
        
        .trades-header {
            display: flex;
            justify-content: space-between;
            align-items: center;
            padding: 16px;
            border-bottom: 1px solid rgba(255, 255, 255, 0.05);
            background-color: #1e1e1e;
        }
        
        .header-left {
            display: flex;
            align-items: center;
            gap: 24px;
        }
        
        .open-trades {
            font-size: 16px;
            font-weight: 600;
        }
        
        .trades-count {
            color: #34D399;
            background-color: rgba(52, 211, 153, 0.1);
            padding: 2px 8px;
            border-radius: 12px;
            margin-left: 8px;
        }
        
        .pnl {
            font-size: 14px;
            color: rgba(255, 255, 255, 0.8);
        }
        
        .pnl-value {
            color: #34D399;
            font-weight: 600;
        }
        
        .pnl-value.negative {
            color: #F87171;
        }
        
        .take-profits-btn {
            background-color: #34D399;
            color: #000;
            font-size: 14px;
            font-weight: 600;
            border: none;
            border-radius: 6px;
            padding: 8px 16px;
            cursor: pointer;
            transition: all 0.2s ease;
            display: flex;
            align-items: center;
            gap: 6px;
        }
        
        .take-profits-btn:hover {
            background-color: #2ebb85;
            transform: translateY(-1px);
        }
        
        .take-profits-btn:before {
            content: "ðŸ’°";
            font-size: 14px;
        }
        
        .filter-row {
            display: flex;
            justify-content: space-between;
            align-items: center;
            padding: 12px 16px;
            border-bottom: 1px solid rgba(255, 255, 255, 0.05);
            background-color: #1c1c1c;
        }
        
        .search-box {
            position: relative;
            flex-grow: 1;
            margin-right: 10px;
        }
        
        .search-icon {
            position: absolute;
            left: 10px;
            top: 50%;
            transform: translateY(-50%);
            color: rgba(255, 255, 255, 0.4);
        }
        
        .search-input {
            background-color: #2a2a2a;
            color: #fff;
            border: 1px solid rgba(255, 255, 255, 0.1);
            border-radius: 6px;
            padding: 8px 10px 8px 34px;
            font-size: 14px;
            width: 100%;
            transition: all 0.2s ease;
        }
        
        .search-input:focus {
            outline: none;
            border-color: #34D399;
            box-shadow: 0 0 0 2px rgba(52, 211, 153, 0.2);
        }
        
        .filter-dropdown {
            display: flex;
            align-items: center;
            gap: 8px;
        }
        
        .filter-label {
            color: rgba(255, 255, 255, 0.7);
            font-size: 14px;
        }
        
        .filter-select {
            background-color: #2c2c2c;
            color: #fff;
            border: 1px solid rgba(255, 255, 255, 0.1);
            border-radius: 6px;
            padding: 8px 12px;
            font-size: 14px;
            cursor: pointer;
            transition: all 0.2s ease;
        }
        
        .filter-select:focus {
            outline: none;
            border-color: #34D399;
        }
        
        .columns-header {
            display: grid;
            grid-template-columns: 1fr 1fr 1fr 1fr 1fr;
            padding: 12px 16px;
            border-bottom: 1px solid rgba(255, 255, 255, 0.05);
            background-color: #212121;
            position: sticky;
            top: 0;
            z-index: 10;
        }
        
        .column-title {
            color: rgba(255, 255, 255, 0.6);
            font-size: 12px;
            font-weight: 600;
            text-transform: uppercase;
            letter-spacing: 0.5px;
        }
        
        .trades-list {
            overflow-y: auto;
            flex-grow: 1;
            padding: 0;
            max-height: calc(100% - 40px);
            scrollbar-width: thin;
            scrollbar-color: rgba(255, 255, 255, 0.1) transparent;
        }            .trades-list::-webkit-scrollbar {
            width: 8px;
        }            .trades-list::-webkit-scrollbar-track {
            background: transparent;
        }            .trades-list::-webkit-scrollbar-thumb {
            background-color: rgba(255, 255, 255, 0.1);
            border-radius: 4px;
        }
        
        .trade-item {
            display: grid;
            grid-template-columns: 1fr 1fr 1fr 1fr 1fr;
            padding: 16px;
            border-bottom: 1px solid rgba(255, 255, 255, 0.05);
            transition: background-color 0.2s ease;
        }
        
        .trade-item:hover {
            background-color: rgba(255, 255, 255, 0.05);
        }
        
        .token-name {
            font-weight: 600;
            color: #fff;
            display: flex;
            align-items: center;
            gap: 8px;
        }
        
        .token-icon {
            width: 20px;
            height: 20px;
            border-radius: 50%;
            background-color: #34D399;
            display: flex;
            justify-content: center;
            align-items: center;
            font-size: 10px;
            color: #000;
        }
        
        .entry-price, .current-price {
            color: rgba(255, 255, 255, 0.8);
        }
        
        .pnl-cell {
            font-weight: 600;
        }
        
        .pnl-cell.positive {
            color: #34D399;
        }
        
        .pnl-cell.negative {
            color: #F87171;
        }
        
        .actions-cell {
            display: flex;
            gap: 6px;
        }
        
        .sell-btn, .view-btn {
