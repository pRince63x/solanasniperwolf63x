    <script>
    // --- THEME SWITCHER ANIMATION LOGIC ---
    (function() {
      const switcher = document.getElementById('theme-switcher');
      const mouth = document.getElementById('theme-mouth');
      const leftEye = document.getElementById('left-eye');
      const rightEye = document.getElementById('right-eye');
      const leftPupil = document.getElementById('left-pupil');
      const rightPupil = document.getElementById('right-pupil');
      let theme = localStorage.getItem('theme') || 'green';
      const body = document.body;          // Helper to animate mouth
      function animateMouth(theme) {
        mouth.classList.remove('smile', 'smile-violet', 'neutral');
        if (theme === 'green') {
          mouth.classList.add('smile');
        } else if (theme === 'violet') {
          mouth.classList.add('smile-violet');
        } else {
          mouth.classList.add('neutral');
        }
      }          // Initial theme
      body.className = `theme-${theme}`;
      animateMouth(theme);          // Theme switch on click
      switcher.addEventListener('click', () => {
        theme = theme === 'green' ? 'violet' : 'green';
        body.className = `theme-${theme}`;
        animateMouth(theme);
        localStorage.setItem('theme', theme);
        // Optionally update charts/colors
        if (window.charts && typeof charts.updateThemeColors === 'function') {
          charts.updateThemeColors();
        }
        // Play mouth bounce
        mouth.classList.remove('smile', 'smile-violet');
        void mouth.offsetWidth;
        animateMouth(theme);
        // Wink right eye for fun
        rightEye.classList.add('wink');
        setTimeout(() => rightEye.classList.remove('wink'), 300);
      });          // Eye tracking logic
      function movePupil(eye, pupil, mouseX, mouseY) {
        const rect = eye.getBoundingClientRect();
        const eyeCenterX = rect.left + rect.width / 2;
        const eyeCenterY = rect.top + rect.height / 2;
        // Calculate vector from eye center to mouse
        let dx = mouseX - eyeCenterX;
        let dy = mouseY - eyeCenterY;
        // Limit distance
        const maxDist = 2.5;
        const dist = Math.sqrt(dx*dx + dy*dy);
        if (dist > maxDist) {
          dx = dx * maxDist / dist;
          dy = dy * maxDist / dist;
        }
        pupil.style.left = (2.5 + dx) + 'px';
        pupil.style.top = (2.5 + dy) + 'px';
      }
      document.addEventListener('mousemove', (e) => {
        movePupil(leftEye, leftPupil, e.clientX, e.clientY);
        movePupil(rightEye, rightPupil, e.clientX, e.clientY);
      });          // On mouse leave, reset pupils to center and blink
      switcher.addEventListener('mouseleave', () => {
        leftPupil.style.left = '2.5px';
        leftPupil.style.top = '2.5px';
        rightPupil.style.left = '2.5px';
        rightPupil.style.top = '2.5px';
        leftEye.classList.add('blink');
        rightEye.classList.add('blink');
        setTimeout(() => {
          leftEye.classList.remove('blink');
          rightEye.classList.remove('blink');
        }, 230);
      });          // Random blinking animation
      function randomBlink() {
        const delay = Math.random() * 3500 + 2000; // 2-5.5s
        setTimeout(() => {
          leftEye.classList.add('blink');
          rightEye.classList.add('blink');
          setTimeout(() => {
            leftEye.classList.remove('blink');
            rightEye.classList.remove('blink');
            randomBlink();
          }, 230);
        }, delay);
      }
      randomBlink();          // On hover, make both eyes look up and mouth bounce
      switcher.addEventListener('mouseenter', () => {
        leftPupil.style.top = '0.5px';
        rightPupil.style.top = '0.5px';
        mouth.style.animation = 'mouth-smile-bounce 0.6s cubic-bezier(.68,-0.55,.27,1.55)';
        setTimeout(() => { mouth.style.animation = ''; }, 600);
      });
    })();
    </script>
    
