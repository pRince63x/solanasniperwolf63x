        /* Animation keyframes enhancements */
        @keyframes eating {
            0%, 100% { height: 1px; transform: scaleY(1); }
            50% { height: 3px; transform: scaleY(1.2); }
        }
        
        @keyframes sleeping {
            0% { opacity: 0; transform: translateY(0) translateX(0) scale(0.8); }
            50% { opacity: 1; transform: translateY(-4px) translateX(4px) scale(1); }
            100% { opacity: 0; transform: translateY(-8px) translateX(8px) scale(0.8); }
        }
        
        @keyframes boxing {
            0% { transform: translateX(0) rotate(0); }
            100% { transform: translateX(5px) rotate(15deg); }
        }
        
        @keyframes shoot {
            0% { opacity: 1; transform: translateX(0) scale(1); }
            100% { opacity: 0; transform: translateX(20px) scale(0.5); }
        }
        
        @keyframes smoke {
            0% { opacity: 0.8; transform: translateY(0) scale(1); }
            100% { opacity: 0; transform: translateY(-10px) scale(2); }
        }
        
        @keyframes swim {
            0%, 100% { transform: translateY(0) rotate(0); }
            25% { transform: translateY(-3px) rotate(3deg); }
            75% { transform: translateY(3px) rotate(-3deg); }
        }
        
        @keyframes swim-arms {
            0% { transform: rotate(-25deg); }
            100% { transform: rotate(25deg); }
        }
        
        @keyframes walk {
            0%, 100% { transform: translateY(0); }
            50% { transform: translateY(-3px); }
        }
        
        @keyframes parachute {
            0%, 100% { transform: translateY(0); }
            50% { transform: translateY(-5px); }
        }
        
        @keyframes gaming-focus {
            0%, 100% { transform: translateY(0) scale(1); }
            50% { transform: translateY(-2px) scale(0.98); }
        }
        
        @keyframes gaming-hands {
            0% { transform: translateX(0); }
            100% { transform: translateX(3px); }
        }
        
        @keyframes jump {
            0%, 100% { transform: translateY(0) scaleY(1); }
            25% { transform: translateY(-2px) scaleY(1.1); }
            50% { transform: translateY(-8px) scaleY(0.9); }
            75% { transform: translateY(-2px) scaleY(1.1); }
        }
        
        @keyframes flip {
            0% { transform: rotateY(0deg); }
            100% { transform: rotateY(360deg); }
        }
        
