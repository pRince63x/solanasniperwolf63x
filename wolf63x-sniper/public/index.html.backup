    <style>
        /* Dashboard styling from dashboard.html */
        .dashboard {
            display: flex;
            height: calc(100vh - 200px);
            width: 100%;
            max-height: 700px;
        }
        
        /* Open Trades Panel */
        .trades-panel {
            width: 40%;
            border-right: 1px solid rgba(255, 255, 255, 0.05);
            display: flex;
            flex-direction: column;
            background-color: #1a1a1a;
            box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
            max-height: 100%;
            overflow: hidden;
        }
        
        .trades-header {
            display: flex;
            justify-content: space-between;
            align-items: center;
            padding: 16px;
            border-bottom: 1px solid rgba(255, 255, 255, 0.05);
            background-color: #1e1e1e;
        }
        
        .header-left {
            display: flex;
            align-items: center;
            gap: 24px;
        }
        
        .open-trades {
            font-size: 16px;
            font-weight: 600;
        }
        
        .trades-count {
            color: #34D399;
            background-color: rgba(52, 211, 153, 0.1);
            padding: 2px 8px;
            border-radius: 12px;
            margin-left: 8px;
        }
        
        .pnl {
            font-size: 14px;
            color: rgba(255, 255, 255, 0.8);
        }
        
        .pnl-value {
            color: #34D399;
            font-weight: 600;
        }
        
        .pnl-value.negative {
            color: #F87171;
        }
        
        .take-profits-btn {
            background-color: #34D399;
            color: #000;
            font-size: 14px;
            font-weight: 600;
            border: none;
            border-radius: 6px;
            padding: 8px 16px;
            cursor: pointer;
            transition: all 0.2s ease;
            display: flex;
            align-items: center;
            gap: 6px;
        }
        
        .take-profits-btn:hover {
            background-color: #2ebb85;
            transform: translateY(-1px);
        }
        
        .take-profits-btn:before {
            content: "ðŸ’°";
            font-size: 14px;
        }
        
        .filter-row {
            display: flex;
            justify-content: space-between;
            align-items: center;
            padding: 12px 16px;
            border-bottom: 1px solid rgba(255, 255, 255, 0.05);
            background-color: #1c1c1c;
        }
        
        .search-box {
            position: relative;
            flex-grow: 1;
            margin-right: 10px;
        }
        
        .search-icon {
            position: absolute;
            left: 10px;
            top: 50%;
            transform: translateY(-50%);
            color: rgba(255, 255, 255, 0.4);
        }
        
        .search-input {
            background-color: #2a2a2a;
            color: #fff;
            border: 1px solid rgba(255, 255, 255, 0.1);
            border-radius: 6px;
            padding: 8px 10px 8px 34px;
            font-size: 14px;
            width: 100%;
            transition: all 0.2s ease;
        }
        
        .search-input:focus {
            outline: none;
            border-color: #34D399;
            box-shadow: 0 0 0 2px rgba(52, 211, 153, 0.2);
        }
        
        .filter-dropdown {
            display: flex;
            align-items: center;
            gap: 8px;
        }
        
        .filter-label {
            color: rgba(255, 255, 255, 0.7);
            font-size: 14px;
        }
        
        .filter-select {
            background-color: #2c2c2c;
            color: #fff;
            border: 1px solid rgba(255, 255, 255, 0.1);
            border-radius: 6px;
            padding: 8px 12px;
            font-size: 14px;
            cursor: pointer;
            transition: all 0.2s ease;
        }
        
        .filter-select:focus {
            outline: none;
            border-color: #34D399;
        }
        
        .columns-header {
            display: grid;
            grid-template-columns: 1fr 1fr 1fr 1fr 1fr;
            padding: 12px 16px;
            border-bottom: 1px solid rgba(255, 255, 255, 0.05);
            background-color: #212121;
            position: sticky;
            top: 0;
            z-index: 10;
        }
        
        .column-title {
            color: rgba(255, 255, 255, 0.6);
            font-size: 12px;
            font-weight: 600;
            text-transform: uppercase;
            letter-spacing: 0.5px;
        }
        
        .trades-list {
            overflow-y: auto;
            flex-grow: 1;
            padding: 0;
            max-height: calc(100% - 40px);
            scrollbar-width: thin;
            scrollbar-color: rgba(255, 255, 255, 0.1) transparent;
        }            .trades-list::-webkit-scrollbar {
            width: 8px;
        }            .trades-list::-webkit-scrollbar-track {
            background: transparent;
        }            .trades-list::-webkit-scrollbar-thumb {
            background-color: rgba(255, 255, 255, 0.1);
            border-radius: 4px;
        }
        
        .trade-item {
            display: grid;
            grid-template-columns: 1fr 1fr 1fr 1fr 1fr;
            padding: 16px;
            border-bottom: 1px solid rgba(255, 255, 255, 0.05);
            transition: background-color 0.2s ease;
        }
        
        .trade-item:hover {
            background-color: rgba(255, 255, 255, 0.05);
        }
        
        .token-name {
            font-weight: 600;
            color: #fff;
            display: flex;
            align-items: center;
            gap: 8px;
        }
        
        .token-icon {
            width: 20px;
            height: 20px;
            border-radius: 50%;
            background-color: #34D399;
            display: flex;
            justify-content: center;
            align-items: center;
            font-size: 10px;
            color: #000;
        }
        
        .entry-price, .current-price {
            color: rgba(255, 255, 255, 0.8);
        }
        
        .pnl-cell {
            font-weight: 600;
        }
        
        .pnl-cell.positive {
            color: #34D399;
        }
        
        .pnl-cell.negative {
            color: #F87171;
        }
        
        .actions-cell {
            display: flex;
            gap: 6px;
        }
        
        .sell-btn, .view-btn {
            background-color: transparent;
            border: 1px solid rgba(255, 255, 255, 0.1);
            border-radius: 4px;
            padding: 4px 8px;
            font-size: 12px;
            cursor: pointer;
            transition: all 0.2s ease;
        }
        
        .sell-btn {
            color: #F87171;
        }
        
        .sell-btn:hover {
            background-color: rgba(248, 113, 113, 0.1);
            border-color: #F87171;
        }
        
        .view-btn {
            color: #60A5FA;
        }
        
        .view-btn:hover {
            background-color: rgba(96, 165, 250, 0.1);
            border-color: #60A5FA;
        }
        
        /* Chart Panel */
        .chart-panel {
            flex-grow: 1;
            display: flex;
            flex-direction: column;
            background-color: #1a1a1a;
            padding: 0;
            max-height: 100%;
            overflow: hidden;
        }
        
        .chart-header {
            display: flex;
            justify-content: space-between;
            align-items: center;
            padding: 16px;
            border-bottom: 1px solid rgba(255, 255, 255, 0.05);
            background-color: #1e1e1e;
        }
        
        .chart-title {
            font-size: 18px;
            font-weight: 600;
            color: #fff;
        }
        
        .chart-value {
            font-size: 18px;
            font-weight: 600;
            color: #34D399;
            background-color: rgba(52, 211, 153, 0.1);
            padding: 6px 12px;
            border-radius: 6px;
        }
        
        .chart-value.negative {
            color: #F87171;
            background-color: rgba(248, 113, 113, 0.1);
        }
        
        .time-controls {
            display: flex;
            padding: 12px 16px;
            border-bottom: 1px solid rgba(255, 255, 255, 0.05);
            background-color: #1c1c1c;
        }
        
        .time-btn {
            background: none;
            border: none;
            color: rgba(255, 255, 255, 0.6);
            font-size: 14px;
            padding: 6px 16px;
            cursor: pointer;
            margin-right: 8px;
            border-radius: 4px;
            transition: all 0.2s ease;
        }
        
        .time-btn:hover {
            background-color: rgba(255, 255, 255, 0.05);
            color: #fff;
        }
        
        .time-btn.active {
            color: #fff;
            background-color: rgba(52, 211, 153, 0.1);
            border-bottom: 2px solid #34D399;
            font-weight: 600;
        }
        
        .chart-container {
            flex-grow: 1;
            padding: 16px;
            position: relative;
            display: flex;
            align-items: center;
            justify-content: center;
            min-height: 200px;
            max-height: calc(100% - 120px);
        }
        
        canvas#profit-chart {
            width: 100% !important;
            height: 100% !important;
        }
        
        .chart-overlay {
            position: absolute;
            top: 0;
            left: 0;
            right: 0;
            bottom: 0;
            display: flex;
            align-items: center;
            justify-content: center;
            background-color: rgba(0, 0, 0, 0.2);
            opacity: 0;
            transition: opacity 0.3s ease;
            pointer-events: none;
        }
        
        .chart-overlay.active {
            opacity: 1;
            pointer-events: auto;
        }
        
        .chart-loading {
            color: #fff;
            font-size: 16px;
            display: flex;
            flex-direction: column;
            align-items: center;
            gap: 12px;
        }
        
        .spinner {
            width: 30px;
            height: 30px;
            border: 3px solid rgba(52, 211, 153, 0.2);
            border-top-color: #34D399;
            border-radius: 50%;
            animation: spin 1s linear infinite;
        }
        
        @keyframes spin {
            to { transform: rotate(360deg); }
        }
        
        .day-labels {
            display: flex;
            justify-content: space-between;
            margin-top: 12px;
            padding: 0 20px;
        }
        
        .day-label {
            color: rgba(255, 255, 255, 0.5);
            font-size: 12px;
        }
        
        .error-message {
            color: #F87171;
            text-align: center;
            margin-top: 20px;
            padding: 12px;
            background-color: rgba(248, 113, 113, 0.1);
            border-radius: 6px;
            display: none;
        }
        
        .empty-container {
            display: flex;
            align-items: center;
            justify-content: center;
            height: 100%;
            color: rgba(255, 255, 255, 0.5);
            font-size: 16px;
            text-align: center;
            padding: 20px;
        }
        
        .chart-legend {
            display: flex;
            justify-content: center;
            gap: 20px;
            margin-top: 12px;
        }
        
        .legend-item {
            display: flex;
            align-items: center;
            gap: 6px;
            font-size: 12px;
            color: rgba(255, 255, 255, 0.7);
        }
        
        .legend-color {
            width: 12px;
            height: 12px;
            border-radius: 2px;
        }
        
        .legend-color.profit {
            background-color: #34D399;
        }
        
        .legend-color.loss {
            background-color: #F87171;
        }
        
        /* Dashboard Summary Stats */
        .dashboard-summary {
            display: grid;
            grid-template-columns: repeat(4, 1fr);
            gap: 16px;
            padding: 16px;
            margin-bottom: 16px;
            background-color: #1a1a1a;
        }
        
        .summary-stat {
            display: flex;
            align-items: center;
            padding: 16px;
            background-color: #212121;
            border-radius: 8px;
            transition: all 0.3s ease;
        }
        
        .summary-stat:hover {
            transform: translateY(-2px);
            box-shadow: 0 6px 12px rgba(0, 0, 0, 0.1);
        }
        
        .stat-icon {
            width: 48px;
            height: 48px;
            border-radius: 12px;
            display: flex;
            justify-content: center;
            align-items: center;
            margin-right: 16px;
            font-size: 20px;
        }
        
        .profit-icon {
            background-color: rgba(52, 211, 153, 0.1);
            color: #34D399;
        }
        
        .volume-icon {
            background-color: rgba(96, 165, 250, 0.1);
            color: #60A5FA;
        }
        
        .trades-icon {
            background-color: rgba(251, 191, 36, 0.1);
            color: #FBBF24;
        }
        
        .balance-icon {
            background-color: rgba(167, 139, 250, 0.1);
            color: #A78BFA;
        }
        
        .stat-info {
            flex-grow: 1;
        }
        
        .stat-label {
            font-size: 12px;
            color: rgba(255, 255, 255, 0.6);
            margin-bottom: 4px;
        }
        
        .stat-value {
            font-size: 18px;
            font-weight: 600;
            margin-bottom: 4px;
        }
        
        .stat-change {
            font-size: 12px;
            display: flex;
            align-items: center;
            gap: 4px;
        }
        
        .positive {
            color: #34D399;
        }
        
        .negative {
            color: #F87171;
        }
        
        /* Quick filters */
        .quick-filters {
            display: flex;
            padding: 8px 16px;
            gap: 8px;
            background-color: #1c1c1c;
            border-bottom: 1px solid rgba(255, 255, 255, 0.05);
        }
        
        .quick-filter {
            background: none;
            border: 1px solid rgba(255, 255, 255, 0.1);
            border-radius: 4px;
            padding: 4px 10px;
            font-size: 12px;
            color: rgba(255, 255, 255, 0.7);
            cursor: pointer;
            transition: all 0.2s ease;
        }
        
        .quick-filter:hover {
            background-color: rgba(255, 255, 255, 0.05);
        }
        
        .quick-filter.active {
            background-color: rgba(52, 211, 153, 0.1);
            border-color: #34D399;
            color: #34D399;
        }
        
        /* Chart controls */
        .chart-controls {
            display: flex;
            justify-content: space-between;
            padding: 0 16px 8px;
            background-color: #1c1c1c;
            border-bottom: 1px solid rgba(255, 255, 255, 0.05);
        }
        
        .chart-control-btn {
            background: none;
            border: 1px solid rgba(255, 255, 255, 0.1);
            border-radius: 4px;
            width: 32px;
            height: 32px;
            display: flex;
            justify-content: center;
            align-items: center;
            color: rgba(255, 255, 255, 0.7);
            cursor: pointer;
            transition: all 0.2s ease;
        }
        
        .chart-control-btn:hover {
            background-color: rgba(255, 255, 255, 0.05);
            color: #fff;
        }
        
        .chart-type-selector {
            display: flex;
            gap: 8px;
        }
        
        .chart-type-btn {
            background: none;
            border: 1px solid rgba(255, 255, 255, 0.1);
            border-radius: 4px;
            width: 32px;
            height: 32px;
            display: flex;
            justify-content: center;
            align-items: center;
            color: rgba(255, 255, 255, 0.7);
            cursor: pointer;
            transition: all 0.2s ease;
        }
        
        .chart-type-btn:hover {
            background-color: rgba(255, 255, 255, 0.05);
            color: #fff;
        }
        
        .chart-type-btn.active {
            background-color: rgba(52, 211, 153, 0.1);
            border-color: #34D399;
            color: #34D399;
        }
        
        /* Responsive adjustments */
        @media (max-width: 1200px) {
            .dashboard-summary {
                grid-template-columns: repeat(2, 1fr);
            }
        }
        
        @media (max-width: 768px) {
            .dashboard {
                flex-direction: column;
                height: auto;
            }
            
            .trades-panel, .chart-panel {
                width: 100%;
                max-height: 500px;
            }
            
            .dashboard-summary {
                grid-template-columns: 1fr;
            }
        }
